<?xml version="1.0" encoding="UTF-8" standalone="no"?>
	<featureModel>
		<properties/>
		<struct>
			<and abstract="true" mandatory="true" name="CryptocalcTask2">
				<feature name="FastExponentiation"/>
				<feature name="EuclideanAlgorithm"/>
				<and name="ExtendedEuclideanAlgorithm">
					<alt abstract="true" mandatory="true" name="Result">
						<feature mandatory="true" name="Show result only"/>
						<feature mandatory="true" name="ShowExtendedSolution"/>
					</alt>
					<or abstract="true" mandatory="true" name="Implementation">
						<feature mandatory="true" name="Iterative"/>
						<feature mandatory="true" name="Recursive"/>
					</or>
				</and>
				<feature name="EulersPhiFunction"/>
				<feature name="CharmichaelsFunction"/>
				<feature name="RandomNumberGenerator"/>
				<feature name="CalculateInExtensionField"/>
				<or name="CheckIfGroupIsCyclic">
					<feature mandatory="true" name="FindPrimitiveRoots"/>
				</or>
				<or abstract="true" name="RSA">
					<feature mandatory="true" name="RSAKeyGeneration"/>
					<feature mandatory="true" name="RSAEncryption"/>
				</or>
				<or abstract="true" name="ElGamal">
					<feature mandatory="true" name="ElGamalKeyGeneration"/>
					<feature mandatory="true" name="ElGamalEncryption"/>
					<feature mandatory="true" name="ElGamalSignature"/>
				</or>
			</and>
		</struct>
		<constraints>
			<rule>
				<imp>
					<var>RSAEncryption</var>
					<var>ExtendedEuclideanAlgorithm</var>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>RSAKeyGeneration</var>
					<conj>
						<var>EulersPhiFunction</var>
						<conj>
							<var>ExtendedEuclideanAlgorithm</var>
							<var>RandomNumberGenerator</var>
						</conj>
					</conj>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>CalculateInExtensionField</var>
					<disj>
						<var>FastExponentiation</var>
						<var>ExtendedEuclideanAlgorithm</var>
					</disj>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>ElGamal</var>
					<conj>
						<var>CheckIfGroupIsCyclic</var>
						<conj>
							<var>FastExponentiation</var>
							<var>FindPrimitiveRoots</var>
						</conj>
					</conj>
				</imp>
			</rule>
		</constraints>
		<calculations Auto="true" Constraints="true" Features="true" Redundant="true" Tautology="true"/>
		<comments/>
		<featureOrder userDefined="false"/>
	</featureModel>
